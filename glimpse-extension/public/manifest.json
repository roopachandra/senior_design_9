  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-6sXlr7DEfrU1hgBgwEG1JqatZ3R4Chcvj7jel4OE2fg='; object-src 'self'",  
  "permissions": [
   "activeTab",
   "storage"
   ]
}

